<div class="small-11 small-centered columns">
  <h2>Users</h2>
  <table id="users-datatable">
    <thead>
      <tr>
        <th>Email Address</th>
        <th>Gets Daily Dharma</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.email %></td>
          <td><%= user.gets_daily_dharma %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
